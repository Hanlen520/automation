 <van-search value="{{ value }}" placeholder="请输入用例编号"  bind:search="onSearch"
/>
 <van-tree-select
  items="{{ items }}"
  main-active-index="{{ mainActiveIndex }}"
  active-id="{{ activeId }}"
  bind:click-nav="onClickNav"
  bind:click-item="onClickItem"
/>
</view>
