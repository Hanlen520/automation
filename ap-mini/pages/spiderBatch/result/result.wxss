/* pages/spiderBatch/result/result.wxss */
.container {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
}
#expect_result {
  position: absolute;
  top: 0;
  bottom: 50%;
  left: 0;
  right: 0;
  border-bottom:2px solid #f57723;
}
 .title{
  margin-left: 30rpx;
  margin-top: 40rpx;
  margin-bottom: 40rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #666;
}

#interface_return {
  position: absolute;
  top: 50%;
  bottom: 0;
  left: 0;
  right: 0;
}
.content text{
  font-size: 28rpx;
  font-weight: 400;
  color: rgba(105, 99, 99, 0.808);
}
.content{
  width: 90%;
  margin-left: auto;
  margin-right: auto;
}
scroll-view{
  width: 100%;
  height: 100%;
}